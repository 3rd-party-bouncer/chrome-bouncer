{
  "manifest_version": 2,

  "name": "Bouncer",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
    "scripts": [ "background.js" ]
	},
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
